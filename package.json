{
  "name": "chatbot",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "start": "docker compose up",
    "dev": "NODE_ENV=development concurrently \"nodemon --exec node --loader ts-node/esm src/server/main.ts\" \"vite\"",
    "build": "tsc && vite build",
    "db": "docker exec -it chatbot_db psql -U postgres",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "prepare": "husky install"
  },
  "prettier": {
    "trailingComma": "es5",
    "semi": false,
    "singleQuote": true
  },
  "lint-staged": {
    "src/**/*.{ts,tsx}": "eslint",
    "*.{ts,tsx,json,md}": "prettier --write"
  },
  "dependencies": {
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "@fontsource/roboto": "^5.0.8",
    "@mui/icons-material": "^5.14.19",
    "@mui/material": "^5.14.19",
    "@tanstack/react-query": "^5.12.1",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "i18next": "^23.7.7",
    "pg": "^8.11.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-i18next": "^13.5.0",
    "react-markdown": "^9.0.1",
    "react-router-dom": "^6.20.1",
    "remark-gfm": "^4.0.0",
    "sequelize": "^6.35.1",
    "ts-node": "^10.9.1",
    "umzug": "^3.4.0",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "@tanstack/react-query-devtools": "^5.12.1",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/node": "^20.10.1",
    "@types/react": "^18.2.37",
    "@types/react-dom": "^18.2.15",
    "@typescript-eslint/eslint-plugin": "^6.10.0",
    "@typescript-eslint/parser": "^6.10.0",
    "@vitejs/plugin-react": "^4.2.0",
    "concurrently": "^8.2.2",
    "eslint": "^8.53.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.4",
    "husky": "^8.0.0",
    "lint-staged": "^15.1.0",
    "nodemon": "^3.0.1",
    "prettier": "3.1.0",
    "typescript": "^5.2.2",
    "vite": "^5.0.0"
  }
}
